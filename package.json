    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "postcss": "8.4.32",
    "autoprefixer": "10.4.16",
    "tailwindcss": "3.3.6",
    "@skeletonlabs/skeleton": "2.6.0",
    "@skeletonlabs/tw-plugin": "0.3.0",
    "vite-plugin-tailwind-purgecss": "0.1.4",
